{
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install",
  "functions": {
    "backend/server.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    }
  ]
}
